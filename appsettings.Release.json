{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "UsedDB": "SQLite",
  "ConnectionStrings": {
    "MySQL": "Persist Security Info=False;server=localhost;port=3306;database=recepten;user id=<USER_NAME>;password=<PASSWORD>;keepalive=10;",
    "SQLite": "/data/recepis.db"
  },
  "JWT" : {
    "JWTKey": "<VERY_LONG_RANDOM_STRING>",
    "TokenLifespan" : {
      "login": 60,
      "verify email": 10,
      "change password": 10
    }
  },
  "PathBase": "/recipes",
  "BaseURL" : "<YOUR_TOP_URL>",
  "AllowedHosts": "<YOUR_DOMAIN>",
  "AllowedOrigins": "<YOUR_DOMAIN>",
  "RedirectHTTPS": "false",
  "MailAddressesRefreshMinutes": 15,
  "FirstUser": {
    "FirstName": "<FIRST_NAME>",
    "LastName": "<LAST_NAME>",
    "UserName": "<USER_NAME>",
    "EMail": "<EMAIL_ADDRESS>"
  }
}
